  </Rule>
  <Rule>
    <Filter>([foo] = 'bar')</Filter>
    <LineSymbolizer stroke-width="11" />
  </Rule>
  <Rule>
    <Filter>([type] = 'primary')</Filter>
    <LineSymbolizer stroke="#8b0000" />
  </Rule>
</Style>